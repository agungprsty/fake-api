                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "description": "Create new post",
                                            "type": "object"
                                        }
                                    },
                                    "type": "object",
                                    "example": {
                                        "uid": 1,
                                        "id": 1,
                                        "title": "Update Title hello world!",
                                        "body": "Update Body hello world!"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "Post"
                ],
                "description": "Delete a post",
