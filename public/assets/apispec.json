    "paths": {
        "/api/posts": {
            "get": {
                "tags": [
                    "Post"
                ],
                "description": "List of post",
                "operationId": "f03c57bdee987a19b68f45422f94fd7c",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "description": "List of post",
                                            "type": "array",
                                            "items": {
                                                "type": "object"
                                            },
                                            "example": [
                                                {
                                                    "uid": 1,
                                                    "id": 1,
                                                    "title": "sunt aut facere repellat provident",
                                                    "body": "quia et suscipit suscipit recusandae consequuntur expedita"
                                                }
                                            ]
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/posts/{id}": {
            "get": {
                "tags": [
                    "Post"
                ],
                "description": "Get by id post",
                "operationId": "3aa8aeb9fe6aafebd9409dd2e819a56a",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Post ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "description": "Find post by id",
                                            "type": "object"
                                        }
                                    },
                                    "type": "object",
                                    "example": {
                                        "uid": 1,
                                        "id": 1,
                                        "title": "sunt aut facere repellat provident",
                                        "body": "quia et suscipit suscipit recusandae consequuntur expedita"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}