    "paths": {
        "/api/posts": {
            "get": {
                "tags": [
                    "Post"
                ],
                "description": "List of post",
                "operationId": "f03c57bdee987a19b68f45422f94fd7c",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "description": "List of post",
                                            "type": "array",
                                            "items": {
                                                "type": "object"
                                            },
                                            "example": [
                                                {
                                                    "uid": 1,
                                                    "id": 1,
                                                    "title": "sunt aut facere repellat provident",
                                                    "body": "quia et suscipit suscipit recusandae consequuntur expedita"
                                                }
                                            ]
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Post"
                ],
                "description": "Create new post",
                "operationId": "4a657e51a0066c64781851f2fd2651e2",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "title": {
                                        "type": "string",
                                        "example": "Title hello world!"
                                    },
                                    "body": {
                                        "type": "string",
                                        "example": "Body Hello world!"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "description": "Create new post",
                                            "type": "object"
                                        }
                                    },
                                    "type": "object",
                                    "example": {
                                        "id": 101,
                                        "title": "Title hello world!",
                                        "body": "Body hello world!"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/posts/{id}": {
            "get": {
                "tags": [
                    "Post"
                ],
                "description": "Get by id post",
                "operationId": "3aa8aeb9fe6aafebd9409dd2e819a56a",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Post ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        },
                        "example": 1
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "description": "Find post by id",
                                            "type": "object"
                                        }
                                    },
                                    "type": "object",
                                    "example": {
                                        "uid": 1,
                                        "id": 1,
                                        "title": "sunt aut facere repellat provident",
                                        "body": "quia et suscipit suscipit recusandae consequuntur expedita"
                                    }
                                }
                            }
                        }
                    }
                }
            },
